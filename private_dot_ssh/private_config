Include ~/.ssh/config.d/*

Host *
	IdentitiesOnly yes
	ServerAliveInterval 60
	Compression yes
	ForwardAgent yes
	StrictHostKeyChecking accept-new
	Port 22
	
Host gaius
	HostName 192.168.1.2
	User RBFocus
	IdentityFile ~/.ssh/vincent

Host github.com github
	Hostname github.com
	User git
	IdentityFile ~/.ssh/vincent
